   "source": [
    "def sqrt(x):\n",
    "    l = 1\n",
    "    r = x\n",
    "    while l <= r:\n",
    "        mid = 1 + (r-1) // 2\n",
    "        if mid*mid > x: r = mid - 1\n",
    "        elif mid*mid < x: l = mid + 1\n",
    "        else: print(mid)\n",
    "    print(r)"
   ]
